{
  "@context" : [
    "https://incf.github.io/neuroshapes/contexts/schema.json",
    {
      "this" : "https://neuroshapes.org/commons/language/shapes/"
    }
  ],
  "@id" : "https://neuroshapes.org/commons/language",
  "@type" : "nxv:Schema",
  "shapes" : [
    {
      "@id" : "this:LanguageShape",
      "@type" : "sh:NodeShape",
      "comment": "A shape to represent natural languages.",
      "property" : [
        {
          "path": "schema:name",
          "name": "Name",
          "description": "The Language name (e.g English).",
          "datatype": "xsd:string"
        },
        {
          "path": "schema:alternateName",
          "name": "Alias",
          "description": "An alias for the language name (e.g English => en).",
          "datatype": "xsd:string"
        }
      ]
    }
  ]
}