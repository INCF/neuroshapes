{
  "@context": [
    "https://incf.github.io/neuroshapes/contexts/schema.json",
    {
      "ex": "http://example.org/"
    }
  ],
  "imports": [
    "https://neuroshapes.org/commons/list/"
  ],
  "@id": "https://neuroshapes.org/commons/list/examples",
  "shapes": [
    {
      "@id": "ex:GeoZoneShape",
      "@type": "sh:NodeShape",
      "label": "Geograpical zone based on zip codes",
      "property": [
        {
          "message": "should contain one list of zip codes as integers",
          "minCount": 1,
          "node": "https://neuroshapes.org/commons/list/shapes/ListOfOneOrMoreIntegersShape",
          "path": "ex:zipCodes"
        },
        {
          "datatype": "xsd:string",
          "message": "should contain a description",
          "minCount": 1,
          "path": "ex:description"
        }
      ],
      "targetClass": "ex:GeoZone"
    },
    {
      "@id": "ex:RecipeShape",
      "@type": "sh:NodeShape",
      "label": "List of ingredients",
      "property": [
        {
          "message": "the list of ingredients must be string",
          "minCount": 1,
          "node": "https://neuroshapes.org/commons/list/shapes/ListOfOneOrMoreStringsShape",
          "path": "ex:ingredients"
        },
        {
          "datatype": "xsd:string",
          "message": "should contain a description",
          "minCount": 1,
          "path": "ex:description"
        }
      ],
      "targetClass": "ex:Recipe"
    },
    {
      "@id": "ex:SyntheticModelShape",
      "@type": "sh:NodeShape",
      "property": [
        {
          "message": "One list for worldMatrix should be present",
          "minCount": 1,
          "node": "ex:WorldMatrixShape",
          "path": "ex:worldMatrix"
        },
        {
          "datatype": "xsd:string",
          "message": "should contain a description",
          "minCount": 1,
          "path": "ex:description"
        }
      ],
      "targetClass": "ex:SyntheticModel"
    },
    {
      "@id": "ex:WhateverShape",
      "@type": "sh:NodeShape",
      "label": "Whatever",
      "property": [
        {
          "datatype": "xsd:string",
          "message": "should contain a description",
          "minCount": 1,
          "path": "ex:description"
        },
        {
          "message": "should contain one list of things",
          "minCount": 1,
          "node": "https://neuroshapes.org/commons/list/shapes/ListOfOneOrMoreItemsShape",
          "path": "ex:things"
        }
      ],
      "targetClass": "ex:Whatever"
    },
    {
      "@id": "ex:WorldMatrixShape",
      "@type": "sh:NodeShape",
      "node": "https://neuroshapes.org/commons/list/shapes/ListOfDoublesShape",
      "property": {
        "maxCount": 16,
        "message": "world matrix is a list of 16 doubles",
        "minCount": 16,
        "path": {
          "oneOrMorePath": "rdf:rest"
        }
      }
    }
  ]
}
