{
  "@context": [
    "https://incf.github.io/neuroshapes/contexts/schema.json",
    {
      "this": "https://neuroshapes.org/commons/list/shapes/"
    }
  ],
  "@id": "nsg:commons/list",
  "@type": "nxv:Schema",
  "shapes": [
    {
      "@id": "this:DoubleNode",
      "not": [
        {
          "hasValue": "rdf:nil"
        }
      ],
      "property": [
        {
          "maxCount": 1,
          "minCount": 1,
          "path": "rdf:rest"
        },
        {
          "path": "rdf:first",
          "message": "Data should be of type double",
          "datatype": "xsd:double",
          "maxCount": 1,
          "minCount": 1
        }
      ]
    },
    {
      "@id": "this:ListOfDoublesNodeShape",
      "@type": "NodeShape",
      "label": "List of doubles node shape",
      "message": "List should contain only doubles",
      "or": [
        "this:DoubleNode",
        "this:EmptyNode"
      ]
    },
    {
      "@id": "this:ListOfDoublesShape",
      "@type": "NodeShape",
      "label": "List of doubles",
      "property": [
        {
          "hasValue": "rdf:nil",
          "node": "this:ListOfDoublesNodeShape",
          "path": {
            "zeroOrMorePath": "rdf:rest"
          }
        }
      ]
    },
    {
      "@id": "this:ListOfOneOrMoreDoublesShape",
      "@type": "NodeShape",
      "node": "this:ListOfDoublesShape",
      "property": [
        {
          "minCount": 1
        }
      ]
    },
    {
      "@id": "this:IntegerNode",
      "not": [
        {
          "hasValue": "rdf:nil"
        }
      ],
      "property": [
        {
          "maxCount": 1,
          "minCount": 1,
          "path": "rdf:rest"
        },
        {
          "datatype": "xsd:integer",
          "maxCount": 1,
          "message": "Data should be integer type",
          "minCount": 1,
          "path": "rdf:first"
        }
      ]
    },
    {
      "@id": "this:ListOfIntegersNodeShape",
      "@type": "NodeShape",
      "label": "List of integers node shape",
      "message": "List should contain only integers",
      "or": [
        "this:IntegerNode",
        "this:EmptyNode"
      ]
    },
    {
      "@id": "this:ListOfIntegersShape",
      "@type": "NodeShape",
      "label": "List of integers",
      "property": [
        {
          "hasValue": "rdf:nil",
          "node": "this:ListOfIntegersNodeShape",
          "path": {
            "oneOrMorePath": "rdf:rest"
          }
        }
      ]
    },
    {
      "@id": "this:ListOfOneOrMoreIntegersShape",
      "@type": "NodeShape",
      "node": "this:ListOfIntegersShape",
      "property": [
        {
          "minCount": 1,
          "path": {
            "oneOrMorePath": "rdf:rest"
          }
        }
      ]
    },
    {
      "@id": "this:ListOfOneOrMoreItemsShape",
      "@type": "NodeShape",
      "node": "this:ListShape",
      "property": [
        {
          "minCount": 1,
          "path": {
            "oneOrMorePath": "rdf:rest"
          }
        }
      ]
    },
    {
      "@id": "this:ListOfOneOrMoreStringsShape",
      "@type": "NodeShape",
      "node": "this:ListOfStringsShape",
      "property": [
        {
          "minCount": 1,
          "path": {
            "oneOrMorePath": "rdf:rest"
          }
        }
      ]
    },
    {
      "@id": "this:ListOfStringsNodeShape",
      "@type": "NodeShape",
      "label": "List of string node shape",
      "message": "List should contain only strings",
      "or": [
        "this:EmptyNode",
        "this:StringNode"
      ]
    },
    {
      "@id": "this:ListOfStringsShape",
      "@type": "NodeShape",
      "label": "List of strings",
      "property": [
        {
          "hasValue": "rdf:nil",
          "node": "this:ListOfStringsNodeShape",
          "path": {
            "oneOrMorePath": "rdf:rest"
          }
        }
      ]
    },
    {
      "@id": "this:EmptyNode",
      "@type": "NodeShape",
      "hasValue": "rdf:nil",
      "property": [
        {
          "maxCount": 0,
          "path": "rdf:rest"
        },
        {
          "maxCount": 0,
          "path": "rdf:first"
        }
      ]
    },
    {
      "@id": "this:DataNode",
      "@type": "NodeShape",
      "not": [
        {
          "hasValue": "rdf:nil"
        }
      ],
      "property": [
        {
          "maxCount": 1,
          "minCount": 1,
          "path": "rdf:rest"
        },
        {
          "maxCount": 1,
          "minCount": 1,
          "path": "rdf:first"
        }
      ]
    },
    {
      "@id": "shsh:ListNodeShape",
      "@type": "NodeShape",
      "description": "Taken from <http://www.w3.org/ns/shacl-shacl#>. Defines constraints on what it means for a node to be a node within a well-formed RDF list. Note that this does not check whether the rdf:rest items are also well-formed lists as this would lead to unsupported recursion.",
      "label": "List node shape",
      "seeAlso": "http://www.w3.org/ns/shacl-shacl#",
      "or": [
        "this:EmptyNode",
        "this:DataNode"
      ]
    },
    {
      "@id": "shsh:ListShape",
      "@type": "NodeShape",
      "description": "Taken from <http://www.w3.org/ns/shacl-shacl#>. A shape describing well-formed RDF lists. Currently does not check for non-recursion. This could be expressed using SHACL-SPARQL.",
      "label": "List shape",
      "seeAlso": "https://www.w3.org/TR/shacl/#syntax-rule-SHACL-list",
      "property": [
        {
          "description": "Each list member (including this node) must be have the shape this:ListNodeShape.",
          "path": {
            "zeroOrMorePath": "rdf:rest"
          },
          "node": "shsh:ListNodeShape",
          "hasValue": "rdf:nil"
        }
      ]
    },
    {
      "@id": "this:StringNode",
      "not": [
        {
          "hasValue": "rdf:nil"
        }
      ],
      "property": [
        {
          "maxCount": 1,
          "minCount": 1,
          "path": "rdf:rest"
        },
        {
          "datatype": "xsd:string",
          "maxCount": 1,
          "message": "Data should be string type",
          "minCount": 1,
          "path": "rdf:first"
        }
      ]
    }
  ]
}
