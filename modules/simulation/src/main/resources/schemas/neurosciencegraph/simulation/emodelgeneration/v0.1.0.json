{
  "@context": [
    "{{base}}/contexts/neurosciencegraph/core/schema/v0.1.0",
    {
      "this": "{{base}}/schemas/neurosciencegraph/simulation/emodelgeneration/v0.1.0/shapes/"
    }
  ],
  "@type": "nxv:Schema",
  "owl:deprecated": true,
  "imports": [
    "{{base}}/schemas/nexus/provsh/generation/v1.0.0",
    "{{base}}/schemas/neurosciencegraph/commons/quantitativevalue/v0.1.0"
  ],
  "shapes": [
    {
      "@id": "this:EModelGenerationShape",
      "@type": "sh:NodeShape",
      "label": "Trace generation shape definition",
      "targetClass": "nsg:TraceGeneration",
      "nodeKind": "sh:BlankNodeOrIRI",
      "and": [
        {
          "node": "{{base}}/schemas/nexus/provsh/generation/v1.0.0/shapes/GenerationShape"
        },
        {
          "property": [
            {
              "path": "prov:activity",
              "name": "Activity",
              "description": "The emodel building activity.",
              "class": "nsg:EModelBuilding",
              "minCount": 1,
              "maxCount": 1
            },
            {
              "path": "nsg:bestScore",
              "name": "Best score",
              "description": "Best score.",
              "node": "{{base}}/schemas/neurosciencegraph/commons/quantitativevalue/v0.1.0/shapes/QuantitativeValueShape",
              "maxCount": 1
            }
          ]
        }
      ]
    }
  ]
}
