{
  "@context": [
    "{{base}}/contexts/neurosciencegraph/core/schema/v0.1.0",
    {
      "this": "{{base}}/schemas/neurosciencegraph/simulation/genbrainactivity/v0.1.0/shapes/"
    }
  ],
  "@type": "nxv:Schema",
  "imports": [
    "{{base}}/schemas/neurosciencegraph/commons/activity/v0.1.4"
  ],
  "shapes": [
    {
      "@id": "this:GenBrainActivityShape",
      "@type": "sh:NodeShape",
      "label": "Generates point neuron network of the scaffold whole brain model.",
      "targetClass": "nsg:GenBrainActivity",
      "nodekind": "sh:BlankNodeOrIRI",
      "and": [
        {
          "node": "{{base}}/schemas/neurosciencegraph/commons/activity/v0.1.4/shapes/ActivityShape"
        },
        {
          "property": [
            {
              "path": "prov:used",
              "name": "Used",
              "description": "Used cell atlas.",
              "class": "nsg:CellAtlas",
              "minCount": 1,
              "maxCount": 1
            },
            {
              "path": "prov:generated",
              "name": "Generated",
              "description": "Generated point neuron network.",
              "class": "nsg:PointNeuronNetwork",
              "minCount": 1,
              "maxCount": 1
            }
          ]
        }
      ]
    }
  ]
}
