{
  "@context": [
    "{{base}}/contexts/neurosciencegraph/core/schema/v0.1.0",
    {
      "this": "{{base}}/schemas/neurosciencegraph/simulation/variablereport/v0.1.0/shapes/"
    }
  ],
  "@type": "nxv:Schema",
  "imports": [
    "{{base}}/schemas/neurosciencegraph/commons/entity/v0.1.0"
  ],
  "owl:deprecated":true,
  "shapes": [
    {
      "@id": "this:VariableReportShape",
      "@type": "sh:NodeShape",
      "label": "Validation report shape",
      "targetClass": "nsg:VariableReportShape",
      "nodekind": "sh:BlankNodeOrIRI",
      "and": [
        {
          "node": "{{base}}/schemas/neurosciencegraph/commons/entity/v0.1.0/shapes/EntityShape"
        },
        {
          "property": [
            {
              "path": "nsg:variable",
              "name": "Variable",
              "description": "Variable shape (e.g: voltage, curent).",
              "datatype": "xsd:string",
              "minCount": 1
            },
            {
              "path": "nsg:target",
              "name": "Target",
              "description": "The variable report target.",
              "datatype": "xsd:string",
              "in": [
                "compartment",
                "soma",
                "summation",
                "extra cellular recording"
              ],
              "minCount": 1
            }
          ]
        }
      ]
    }
  ]
}
